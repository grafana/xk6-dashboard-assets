    "@vanilla-extract/vite-plugin": "5.1.4",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.5",
    "rollup-plugin-visualizer": "6.0.5",
    "sass": "1.97.3",
    "vite": "7.2.2",
    "vite-plugin-handlebars": "2.0.0",
